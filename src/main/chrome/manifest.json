{
  "name": "Skip",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "This extension provides an in-browser view of your Singlehop assets.  It also allows quick start/stop control of VMs",  
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "background": {
    "scripts": [
      "bg/background.js"
    ],
    "persistent": true
  },
  "options_page": "src/options/index.html",
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "Singlehop Viewer",
    "default_popup": "src/main/index.html"
  },
  "permissions": [
    "https://dropzone.singlehop.com/*",
    "storage"
    
  ]
}